            <LinearGradient
              colors={isDark ? ['#880E4F', '#C2185B'] : ['#880E4F', '#C2185B']}
              style={styles.announcementCard}
              start={{ x: 0, y: 0 }}
              end={{ x: 1, y: 1 }}
